<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" 
	"http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
	<settings>
		<!-- changes from the defaults -->
		<setting name="lazyLoadingEnabled" value="false" />
		<!-- JdbcType enumeration. Most common are: NULL, VARCHAR and OTHER -->
		<setting name="jdbcTypeForNull" value="NULL" />
		<setting name="defaultStatementTimeout" value="600" />
	</settings>
	<mappers>
		<!-- fill the mappers -->
		<mapper resource="mapper/TaskLogMapper.xml"/>
		<mapper resource="mapper/ClientTaskControlMapper.xml"/>
		<mapper resource="mapper/ClientTaskStatusLogMapper.xml"/>
		<mapper resource="mapper/PhysicDelRecordMapper.xml"/>
		<mapper resource="mapper/TabDupPrimaryConfigMapper.xml"/>
		<mapper resource="mapper/ApplicationInfoMapper.xml"/>
		<mapper resource="mapper/ResumeHiveTaskInfoMapper.xml"/>
	</mappers>
</configuration>
